{
  "manifest_version": 3,
  "name": "Eyer",
  "version": "0.1.0",
  "description": "Relieve eye strain",
  "background": {
        "service_worker": "background.js"
      },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["notifications", "alarms", "storage"],
  "icons": {
    "128": "eye128.png"
  }
}
